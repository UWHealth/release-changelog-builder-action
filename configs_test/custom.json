{
  "template": "<details><summary><strong>Stats from previous release: ${{FROM_TAG}}, #{{CATEGORIZED_COUNT}} total pull requests</strong></summary><p>\n\n**Compare**: ${{RELEASE_DIFF}}<br/>**Changed Files:** ${{CHANGED_FILES}}<br/>**New Lines:** ${{ADDITIONS}}<br/>**Deleted Lines:** ${{DELETIONS}}<br/>**Changed Lines:** ${{CHANGES}}</p></details>\n\n# Changes\n\n${{CHANGELOG}}",
  "sort": {
    "order": "DESC",
    "on_property": "mergedAt"
  },
  "pr_template": "<details><summary><strong>${{TITLE}} <a href=\"${{URL}}\">(#${{NUMBER}})</a></strong></summary><p>\n\nStarted by: @#{{AUTHOR}}<br/>Approvers: #{{APPROVERS_LIST}}<br/></p><blockquote><details><summary>‚Ñπ PR Details</summary>\n\n#{{BODY}}</blockquote></details></details>",
  "tag_resolver": {
    "filter": {
      "pattern": "v[\\d]+\\.[\\d]+\\.[\\d]+"
    }
  },
  "custom_placeholders": [
    {
      "name": "APPROVERS_LIST",
      "source": "APPROVERS",
      "transformer": {
        "pattern": "([^,\\s][^,]*[^,\\s]*)",
        "target": "@$1"
      }
    }
  ],
  "categories": [
    {
      "title": "### üí• Breaking",
      "labels": [
        "!",
        "`",
        "break",
        "breaking",
        "Break",
        "Breaking"
      ]
    },
    {
      "title": "### üî• Hot",
      "labels": [
        "hot",
        "Hot"
      ]
    },
    {
      "title": "### üß¨ Content Model Changes",
      "labels": [
        "content model change",
        "functions"
      ]
    },
    {
      "title": "### üöÄ Enhancements",
      "labels": [
        "feat",
        "Feat",
        "enhancement",
        "functions"
      ]
    },
    {
      "title": "### üêõ Defects",
      "labels": [
        "fix",
        "Fix",
        "bug"
      ]
    },
    {
      "title": "### üì¶ Dependencies",
      "labels": [
        "dependencies"
      ]
    },
    {
      "title": "### üí¨ Uncategorized",
      "labels": []
    }
  ],
  "label_extractor": [
    {
      "pattern": "(\\w+)[:\\(].*",
      "target": "$1",
      "on_property": "title"
    },
    {
      "pattern": ".*(`).*",
      "target": "$1",
      "on_property": "title"
    },
    {
      "pattern": "(\\w+)\\(([Hh]ot)\\)[:\\(].*",
      "target": "$2",
      "on_property": "title"
    }

  ],
  "base_branches": [
    "develop"
  ],
  "outputFile": "CHANGELOG.md"
}